* text=auto
* text eol=lf
# Archive ignore
/.github export-ignore
/.static export-ignore
/gui export-ignore
.gitignore export-ignore
.gitattributes export-ignore
*.yml export-ignore
*.sh export-ignore
*.po export-ignore
*.code-workspace export-ignore
*.png filter=lfs diff=lfs merge=lfs -text
* !text !filter !merge !diff
